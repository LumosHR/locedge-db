{
    "ArvanCloud": {
        "header": "ar-sid",
        "regex": "^([0-9]{4})$"
    },
    "Cloudflare": {
        "header": "cf-ray",
        "regex": "^.*-([a-z]{3})$"
    },
    "Beluga": {
        "header": "x-beluga-node",
        "regex": "^(.*)$"
    },
    "CDN77": {
        "header": "x-77-pop",
        "regex": "^([a-z0-9]+)$"
    },
    "CloudFront": {
        "header": "x-amz-cf-pop",
        "regex": "^([a-z]{3}).*$"
    },
    "KeyCDN": {
        "header": "x-edge-location",
        "regex": "^([a-z0-9]{4})$"
    },
    "G-Core": {
        "header": "x-id",
        "regex": "([a-z0-9]+)-[a-z0-9]+-[a-z0-9]+"
    },
    "CacheFly": {
        "header": "x-cf1",
        "regex": "[^.]*\\.([a-z]{3}).?[:-]"
    },
    "Fastly": {
        "header": "x-served-by",
        "regex": "cache-?[a-z0-9]*-([a-z]{3})[a-z0-9]+-([a-z]{3}),?"
    },
    "StackPath": {
        "header": "x-hw",
        "regex": "[0-9]+\\.[a-z0-9]+\\.([a-z]+).?\\.[a-z0-9]+,?"
    },
    "Sucuri": {
        "header": "x-sucuri-id",
        "regex": "^([0-9]{2})[0-9]+$"
    },
    "Bunny": {
        "header": "server",
        "regex": "bunnycdn-([a-z]{2,3}[0-9]*)-.*"
    },
    "Edgecast": {
        "header": "server",
        "regex": "^[a-z]+\\s\\(([a-z]{3}).*\\)$"
    },
    "Tata": {
        "header": "x-cache",
        "regex": "[^,]*,v[0-9]+([a-z]{3}).*"
    }
}